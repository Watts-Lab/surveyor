{
  "name": "surveyor",
  "version": "0.0.1",
  "description": "A tool for data driven surveys",
  "main": "built/app.js",
  "scripts": {
    "start": "node built/app.js",
    "compile": "tsc -p .",
    "test": "tsc -w",
    "dev": "nodemon built/app.js",
    "start_mongo": "sudo service mongodb start",
    "stop_mongo": "sudo service mongodb stop",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "dependencies": {
    "@types/csurf": "^1.11.2",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parse": "^4.15.4",
    "csv-stringify": "^5.6.2",
    "dotenv": "^10.0.0",
    "express": "^4",
    "express-session": "^1.17.2",
    "googleapis": "^74.2.0",
    "js-base64": "^3.7.2",
    "jsonwebtoken": "^8.5.1",
    "jstransformer-markdown-it": "^2.1.0",
    "marked": "^2.0.6",
    "mongodb": "^4.1.2",
    "node-fetch": "^2.6.1",
    "pug": "^3.0.2",
    "readline": "^1.3.0",
    "request": "^2.88.2",
    "svg-captcha": "^1.4.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.6",
    "@types/express-session": "^1.17.4",
    "@types/marked": "^2.0.4",
    "@types/node": "14.0.13",
    "@types/node-fetch": "^2.5.11",
    "@types/request": "^2.48.6",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "prettier": "^2.5.1",
    "typescript": "^4.3.5"
  },
  "repository": {
    "url": "https://github.com/Watts-Lab/surveyor"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "data-driven-survey",
    "express"
  ]
}
